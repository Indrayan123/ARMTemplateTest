{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApimServiceName": {
      "type": "string"
    }
  },
  "resources": [
    {
      "properties": {
        "description": "testproduct",
        "subscriptionRequired": true,
        "approvalRequired": false,
        "state": "published",
        "displayName": "TestProduct"
      },
      "name": "[concat(parameters('ApimServiceName'), '/testproduct')]",
      "type": "Microsoft.ApiManagement/service/products",
      "apiVersion": "2017-05-10"
    },
    {
      "properties": {
        "description": "Administrators is a built-in group. Its membership is managed by the system. Microsoft Azure subscription administrators fall into this group.",
        "displayName": "Administrators",
        "type": "system",
        "builtIn": true
      },
      "name": "[concat(parameters('ApimServiceName'), '/testproduct/administrators')]",
      "type": "Microsoft.ApiManagement/service/products/groups",
      "apiVersion": "2017-05-10",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/products', parameters('ApimServiceName'), 'testproduct')]"
      ]
    },
    {
      "properties": {
        "description": "Developers is a built-in group. Its membership is managed by the system. Signed-in users fall into this group.",
        "displayName": "Developers",
        "type": "system",
        "builtIn": true
      },
      "name": "[concat(parameters('ApimServiceName'), '/testproduct/developers')]",
      "type": "Microsoft.ApiManagement/service/products/groups",
      "apiVersion": "2017-05-10",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/products', parameters('ApimServiceName'), 'testproduct')]"
      ]
    }
   
  ]
}